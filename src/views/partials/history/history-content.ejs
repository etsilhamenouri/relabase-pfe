<div class="main-header-container">
    <div class="main-header">Script history</div>
    <div class="search-inputs">
        <div class="date-from-input-container">
            <input type="text" class="search-input" id="history-date-from-search"
                placeholder="Search from date (YYYY/MM/DD)" pattern="\d{4}/\d{2}/\d{2}">
        </div>
        <div class="date-to-input-container">
            <input type="text" class="search-input" id="history-date-to-search"
                placeholder="Search to date (YYYY/MM/DD)" pattern="\d{4}/\d{2}/\d{2}">
        </div>
        <div class="search-input-container">
            <input type="text" class="search-input" id="history-search-from" placeholder="Search from">
        </div>
    </div>
</div>